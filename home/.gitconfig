[user]
	email = gaithimo@gmail.com
	name = gaishimo
[include]
  path = .gitconfig.local
[color]
	ui = true
[core]
  editor = vim
  excludesfile = /opt/boxen/config/git/gitignore
	precomposeunicode = true
[pager]
	diff = diff-highlight | less
	log = diff-highlight | less
	show = diff-highlight | less

[hub]
protocol = https

[alias]
  st=status
  sh=show
  cm=commit
  co=checkout
	delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
  unstage = reset -q HEAD --
  discard = checkout --
  uncommit = reset --mixed HEAD~
  amend = commit --amend
  graph = log --graph -10 --branches --remotes --tags  --format=format:'%Cgreen%h %Cresetâ€¢ %<(75,trunc)%s (%cN, %cr) %Cred%d' --date-order
[ghn]
	token = d34e620820d8964e3fd299a3edbebe460555e8b8
[push]
	default = simple

[alias]
  reamend = commit --amend -C HEAD
[diff]
	compactionHeuristic = true
